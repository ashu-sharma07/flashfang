<%- include('header'); -%>
<div class="card">
  <div class="card-heading-box">
    <div class="card-heading">English Word</div>
  </div>
  <div class="word"><%= englishWord %> </div>
</div>
<div class="user-form">
  <form action="/" method="post">
    <input
      type="text"
      name="userWord"
      id=""
      autocomplete="off"
      placeholder="sap"
      class="userInput"
      >
    <div class="check-solution">
      <button type="submit" class="btn">Check Solution</button>
    </div>
  </form>
</div>
<%- include('footer'); -%>
